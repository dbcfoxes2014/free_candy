<h1>Search results for '#<%=@search_content%>'</h1>

<% @media.each do |media| %>
  <% if media.type == "image" %>
    <img src="<%=media.images.standard_resolution.url%>">
  <% elsif media.type == "video" %>
	<div id="wrapper">
	    <div class="video-thumbnail">
	    	<img width="260" height="260" src='<%= media.thumbnail.url %>'>
		</div>
	</div>





  <% end %>
<% end %>


